<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<deviceModel maxAgeSeconds="1800" xmlns="urn:teleal-org:cling:bridge:combined-descriptor-1-0">
    <root xmlns="urn:schemas-upnp-org:device-1-0">
        <specVersion>
            <major>1</major>
            <minor>0</minor>
        </specVersion>
        <device>
            <deviceType>urn:schemas-upnp-org:device:MY-DEVICE-TYPE:1</deviceType>
            <UDN>uuid:MY-DEVICE-123</UDN>
            <friendlyName>My Testdevice</friendlyName>
            <iconList>
                <icon>
                    <mimetype>image/png</mimetype>
                    <width>48</width>
                    <height>48</height>
                    <depth>8</depth>
                    <url>icon.png</url>
                </icon>
                <icon>
                    <mimetype>image/png</mimetype>
                    <width>48</width>
                    <height>48</height>
                    <depth>8</depth>
                    <url>icon2.png</url>
                </icon>
            </iconList>
            <serviceList>
                <service>
                    <serviceType>urn:schemas-upnp-org:service:MY-SERVICE-TYPE-ONE:1</serviceType>
                    <serviceId>urn:upnp-org:serviceId:MY-SERVICE-123</serviceId>
                    <controlURL>/dev/MY-DEVICE-123/svc/upnp-org/MY-SERVICE-123/action</controlURL>
                    <eventSubURL>/dev/MY-DEVICE-123/svc/upnp-org/MY-SERVICE-123/event</eventSubURL>
                    <SCPDURL>/dev/MY-DEVICE-123/svc/upnp-org/MY-SERVICE-123/desc.xml</SCPDURL>
